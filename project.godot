config_version=4

_global_script_classes=[ {
"base": "AnimatedAtlas",
"class": "Actor",
"language": "GDScript",
"path": "res://scripts/Actor.gd"
}, {
"base": "Sprite",
"class": "AnimatedAtlas",
"language": "GDScript",
"path": "res://scripts/AnimatedAtlas.gd"
}, {
"base": "Node",
"class": "SongEvent",
"language": "GDScript",
"path": "res://scripts/Event.gd"
}, {
"base": "Node",
"class": "SongScript",
"language": "GDScript",
"path": "res://scripts/Song.gd"
} ]
_global_script_class_icons={
"Actor": "",
"AnimatedAtlas": "",
"SongEvent": "",
"SongScript": ""
}

[application]

config/name="AkuraEngine"
run/main_scene="res://scenes/Startup.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="akura-engine"
run/delta_smoothing=false
config/icon="res://icon.png"

[autoload]

VolumeManager="*res://scenes/autoload/VolumeManager.tscn"
SoundManager="*res://scenes/autoload/SoundManager.tscn"
SceneManager="*res://scenes/autoload/SceneManager.tscn"
ModsManager="*res://scripts/autoload/ModsManager.gd"
DebugOverlay="*res://scenes/autoload/DebugOverlay.tscn"
AtlasParser="*res://scripts/autoload/AtlasParser.gd"
ChartParser="*res://scripts/autoload/ChartParser.gd"
Conductor="*res://scripts/autoload/Conductor.gd"
Settings="*res://scripts/autoload/Settings.gd"
Globals="*res://scripts/autoload/Globals.gd"
Scores="*res://scripts/autoload/Scores.gd"
Status="*res://scripts/autoload/Status.gd"
Temp="*res://scripts/autoload/Temp.gd"
Math="*res://scripts/autoload/Math.gd"
Ref="*res://scripts/autoload/Ref.gd"

[display]

window/size/width=1280
window/size/height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

